<h4 class="user-marketplace">Browse User Marketplace</h4>
<hr class="hero-separator pink" />

<section class="marketplace">

    <div class="img-container">

        <a asp-area="Marketplace" asp-controller="Store" asp-action="Explore">
            <div class="marketplace-index-image explore">
                <span class="marketplace-names explore">Explore Marketplace</span>
                <img id="explore-img" class="marketplace-img" src="~/images/index-page/explore-marketplace-index.jpg" />
            </div>
        </a>

        <a asp-area="Marketplace" asp-controller="Product" asp-action="Add">
            <div class="marketplace-index-image create">
                <span class="marketplace-names create">Create Listing</span>
                <img id="create-img" class="marketplace-img" src="~/images/index-page/create-listing-index.jpg" />
            </div>
        </a>

    </div>

</section>

<script src="~/js/home-index/index-script.js"></script>