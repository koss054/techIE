@model IEnumerable<CategoryViewModel>
@{
    ViewData["Title"] = "Admin - Categories";
}

<head>
    <link rel="stylesheet" href="~/css/button-style.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/admin/admin-panel.css" asp-append-version="true" />
</head>

<div class="panel">
    <a asp-area="Admin" asp-controller="Panel" asp-action="Index">
        <img class="return-to-panel grow" src="~/images/menus/left-arrow-white.svg" />
    </a>
    <img src="~/images/admin/admin-panel.jpg" />
    <div class="panel-inputs">
        <div class="category-table">
            <partial name="~/Areas/Admin/Views/Category/_CategoryTablePartial.cshtml" />
        </div>
        
    </div>
</div>

<script src="~/js/alert/confrimation.js" asp-append-version="true"></script>